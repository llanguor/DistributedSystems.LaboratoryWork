<Window x:Class="DistributedSystems.LaboratoryWork.Number1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DistributedSystems.LaboratoryWork.Number1"
        xmlns:system="clr-namespace:System;assembly=mscorlib"
        xmlns:attached="clr-namespace:DistributedSystems.LaboratoryWork.Number1.Packages.Attached"
        xmlns:controls="clr-namespace:DistributedSystems.LaboratoryWork.Number1.Packages.Controls"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000"
        Loaded="MainWindow_OnLoaded"
        WindowStartupLocation="CenterScreen">


    <TabControl x:Name="tabControl">
        <TabItem Header="Buttons">
            <Frame x:Name="buttonsFrame"/>
        </TabItem>
        <TabItem Header="NumericKeyboard">
            <Frame x:Name="numericKeyboardFrame"/>
        </TabItem>
        <TabItem Header="LetterKeyboard">
            <Frame x:Name="letterKeyboardFrame"/>
        </TabItem>
    </TabControl>
</Window>